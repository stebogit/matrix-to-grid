    "@turf/truncate": "*",
    "benchmark": "*",
    "eslint": "*",
    "eslint-config-mourner": "*",
    "glob": "*",
    "load-json-file": "*",
    "rollup": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-uglify": "*",
    "tape": "*",
    "write-json-file": "*"
  },
  "dependencies": {
